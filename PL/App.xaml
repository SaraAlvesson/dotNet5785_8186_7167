<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="LoginWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <local:CallTypeCollection x:Key="CallTypeCollectionKey" />
            <local:PositionCollection x:Key="PositionCollectionKey" />
            <local:DistanceTypeCollection x:Key="DistanceTypeCollectionKey" />
            <local:StatusTypeCollection x:Key="StatusTypeCollectionKey" />
            <local:CallStatusToVisibilityConverter x:Key="CallStatusToVisibilityConverter"/>
            <!--<local:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>-->
        </ResourceDictionary>
    </Application.Resources>
</Application>
